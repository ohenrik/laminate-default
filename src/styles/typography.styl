html, body {
  font-family: 'Lato', sans-serif;
  font-size: 12pt;
  font-weight: normal;
  color: #333;
}
p, blockquote {
  margin-bottom: 1.25em;
  line-height: 1.25em;
  prince-footnote-policy: keep-with-line;
}
sup {
  vertical-align: super;
  font-size: smaller;
}
h1,h2,h3,h4,h5,h6 {
  font-family: 'Lato', sans-serif;
  font-weight: normal;
  color: #333;
  margin-bottom: 0.5em;
}
h1 {
  font-size: 30px;
}
h2 {
  font-size: 24px;
}
h3 {
  font-size: 16px;
}
h4 {
  font-size: 12px;
  font-weight: bold;
}
